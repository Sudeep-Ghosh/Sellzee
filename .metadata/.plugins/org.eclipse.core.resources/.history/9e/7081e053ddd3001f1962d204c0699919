package com.sellzee.authService.controller;

public class Fibonacci {

	public static long fibonacci(int n) {
			if (n <= 1) {
            return n;
        } else {
            return fibonacci(n - 1) + fibonacci(n - 2);
        }
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

}
